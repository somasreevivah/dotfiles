
LIBPATH=${LIBPATH}:~/Documents/

PDF_VIEWER="zathura --fork"
PS_VIEWER="zathura --fork"
TEXT_VIEWER="vim"
PDF_VIEWER="zathura --fork"
CONFIG_FILE="$HOME/.librc"
VIDEO_VIEWER="mpv"
MUSIC_VIEWER="mplayer"



registerCommand papers
papers_PATH=~/Documents/marcus_notes/



registerCommand soft
soft_PATH=~/software/

registerCommand vid
vid_PATH=~/Videos/

registerCommand his
his_PATH=~/.bash_history
his_SEARCHER=historySearcher
his_ACTION=historyAction

historySearcher(){
  cat $his_PATH \
    | sed '/^[\.-]*$/d'\
    | sed '/^\s*$/d'\
    | sed '/#.*/d'\
    | sort \
    | uniq
}


