# scroll motion
keymap C-f NEXT_PAGE
keymap C-b PREV_PAGE
keymap C-d NEXT_PAGE
keymap C-u PREV_PAGE
keymap C-e UP
keymap C-y DOWN
keymap zz CENTER_V
keymap gg BEGIN
keymap G END

# Cursor motion
keymap l MOVE_RIGHT
keymap h MOVE_LEFT
keymap j MOVE_DOWN
keymap k MOVE_UP
keymap < SHIFT_LEFT
keymap > SHIFT_RIGHT
keymap 0 LINE_BEGIN
keymap $ LINE_END
keymap w NEXT_WORD
keymap b PREV_WORD

########## cursor history ##########
keymap C-i REDO
keymap C-o UNDO

########## navigation ##########
keymap f MOVE_LIST_MENU
keymap F LIST_MENU
keymap J NEXT_LINK
keymap K PREV_LINK
#keymap C-n NEXT_LINK
#keymap C-p PREV_LINK

# Hyperlink operation
keymap M MARK_WORD
# keymap M  MARK_URL
keymap C-t TAB_LINK
keymap C-s SAVE_LINK
keymap V  VIEW_IMAGE

# File/Stream operation
keymap o GOTO
keymap @ READ_SHELL
keymap |   PIPE_BUF

########## info ##########
keymap y PEEK
keymap C-g LINE_INFO
keymap gf VIEW

########## history navigation ##########
keymap L NEXT
keymap H PREV

# Buffer operation
keymap B BACK
keymap ESC-s SAVE
keymap P PRINT
keymap r RELOAD
#keymap E EDIT
keymap E EDIT_SCREEN

########## tab ##########
keymap d CLOSE_TAB
#keymap gh GOTO http://www.google.com/en
keymap t TAB_GOTO
keymap C-p PREV_TAB
keymap C-n NEXT_TAB
keymap C-wL TAB_RIGHT
keymap C-wH TAB_LEFT
keymap C-t NEW_TAB

keymap ESC-b VIEW_BOOKMARK
keymap ESC-a ADD_BOOKMARK

# Search 
keymap / ISEARCH
keymap ? ISEARCH_BACK
keymap n SEARCH_NEXT
keymap N SEARCH_PREV

# Dictionay look-up
keymap ESC-w DICT_WORD
keymap ESC-W DICT_WORD_AT

# Mark operation
keymap m MARK
keymap ] NEXT_MARK
keymap [ PREV_MARK
keymap  \"  REG_MARK

# bookmark operation
keymap ESC-b VIEW_BOOKMARK
keymap ESC-a ADD_BOOKMARK

# Misc operation
keymap \\ MENU
keymap H HELP
keymap ESC-o OPTIONS
keymap ESC-k COOKIE
keymap ESC-h HISTORY
keymap : COMMAND
keymap ! EXEC_SHELL
keymap ESC-z INTERRUPT
keymap C    CHARSET

# Leader key
keymap ,h SELECT_MENU
keymap ,q QUIT
keymap ,b TAB_MENU
