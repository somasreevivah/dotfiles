"3.10.1 (created: 2015/08/25 00:41:59)

source! $HOME/.vimperatorrc.local


" go tp the last tab opened for toggling
noremap g; <C-^>
inoremap <C-c> <Esc>
nnoremap J :tabnext<cr>
nnoremap K :tabprev<cr>

nnoremap ,s :source ~/.vimperatorrc<cr>

nnoremap ,p :videocontrol playOrPause<cr>
nnoremap ,f :videocontrol fullscreen<cr>
nnoremap ,h :videocontrol shift -10<cr>
nnoremap ,l :videocontrol shift 10<cr>


" vim: set ft=vim:
