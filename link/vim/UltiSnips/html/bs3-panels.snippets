extends html


snippet bs3-panel:danger "" b

<div class="panel panel-${1:danger}">
	<div class="panel-heading">
		<h3 class="panel-title">Panel title</h3>
	</div>
	<div class="panel-body">
		${2:Panel content}
	</div>
</div>

endsnippet


snippet bs3-panel:footer "" b

<div class="panel panel-default">
	<div class="panel-body">
		${1:Panel content}
	</div>
	<div class="panel-footer">
		${2:Panel footer}
	</div>
</div>

endsnippet


